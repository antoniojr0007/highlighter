<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.highlighter.www.highlighter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:background="@drawable/fundo"
        android:icon="@drawable/fundo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fundo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CadastroActivity" />
        <activity android:name=".Activity.AtualizarPerfilActivity" />
        <activity android:name=".Activity.LivroActivity" />
        <activity android:name=".Activity.ComentarioLivroActivity" />
        <activity android:name=".Activity.FilmeActivity" />
        <activity android:name=".Activity.SerieActivity" />
        <activity android:name=".Activity.ComentarioFilmeActivity" />
        <activity android:name=".Activity.ComentarioSerieActivity" />
        <activity android:name=".Activity.SugestaoLivroActivity" />
        <activity android:name=".Activity.SugestaoFilmeActivity" />
        <activity android:name=".Activity.SugestaoSerieActivity"></activity>
    </application>

</manifest>